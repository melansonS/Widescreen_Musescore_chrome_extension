{
  "manifest_version": 2,

  "name": "Musescore Widescreen",
  "description": "Removes unnecessary html elements to give the sheet music more room",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  
  "browser_specific_settings": {
    "gecko": {
    "id": "addon@example.com",
    "strict_min_version": "42.0"
    }
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
